    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch CurrencyConversionServiceApplication",
            "request": "launch",
            "mainClass": "com.hassane.currencyconversionservice.CurrencyConversionServiceApplication",
            "projectName": "currency-conversion-service"
        },
        {
            "type": "java",
            "name": "Launch Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Launch CurrencyExchangeServiceApplication8000",
            "request": "launch",
            "mainClass": "com.hassane.currencyexchangeservice.CurrencyExchangeServiceApplication",
            "projectName": "currency-exchange-service",
            "vmArgs": "-Dserver.port=8000"
        },
        {
            "type": "java",
            "name": "Launch CurrencyExchangeServiceApplication 8001",
            "request": "launch",
            "mainClass": "com.hassane.currencyexchangeservice.CurrencyExchangeServiceApplication",
            "projectName": "currency-exchange-service",
            "vmArgs": "-Dserver.port=8001"
        },
        {
            "type": "java",
            "name": "Launch LimitsServiceApplication",
            "request": "launch",
            "mainClass": "com.hassane.limitsservice.LimitsServiceApplication",
            "projectName": "limits-service"
        },
        {
            "type": "java",
            "name": "Launch SpringCloudConfigServerApplication",
            "request": "launch",
            "mainClass": "com.hassane.microservices.springcloudconfigserver.SpringCloudConfigServerApplication",
            "projectName": "spring-cloud-config-server"
        }
    ]
}